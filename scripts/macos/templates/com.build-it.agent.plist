<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.build-it.agent</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/libexec/BUILD_IT_AGENT/build-it-agent</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>~/Library/Logs/build-it-agent.out.log</string>
  <key>StandardErrorPath</key>
  <string>~/Library/Logs/build-it-agent.err.log</string>
</dict>
</plist>
